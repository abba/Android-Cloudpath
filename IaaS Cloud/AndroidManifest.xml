<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="cloud.path"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="8" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS"></uses-permission>
    <uses-permission android:name="android.permission.CONTROL_LOCATION_UPDATES"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application android:label="@string/app_name" android:icon="@drawable/myicon">
        <activity android:name=".CloudPath_Main"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="Mgmt"></activity>
        <activity android:name="IaaS"></activity>
        <activity android:name="About">
        </activity>
        <activity android:name="Evaluate" android:multiprocess="true"></activity>
        <activity android:name="Result"></activity>
        <activity android:name="Login"></activity>
        <activity android:name="Register"></activity>
        <activity android:name="Location" android:theme="@android:style/Theme.NoTitleBar"></activity>
       
        <activity android:name="Maps"></activity>

    </application>
</manifest>